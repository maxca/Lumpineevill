{
    "name": "samark/lumpineevill",
    "description": "Laravel Generator , For api base CRUD , Management request , handler response with JSON.",
    "type": "library",
    "license": "MIT",
    "authors": [
    {
        "name": "Samark Chaisanguan",
        "email": "samarkchsngn@gmail.com"
    }],
    "minimum-stability": "stable",
    "require":
    {
        "php": ">=5.3.0",
        "illuminate/support": ">=5.3.0",
        "guzzlehttp/guzzle": ">=5.0.0"
    },
    "require-dev":
    {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload":
    {
        "classmap": [
            "src/"
        ],
        "psr-0":
        {
            "Samark\\Lumpineevill\\": "src/"
        },
        "files": [
            "src/Helper/curl.php"
        ]
    },
    "autoload-dev":
    {
        "psr-0":
        {
            "Samark\\Lumpineevill\\Tests\\": "tests"
        }
    }
}